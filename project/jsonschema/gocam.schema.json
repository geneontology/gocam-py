            "type": "object"
        },
        "CausalAssociation": {
            "additionalProperties": false,
            "description": "A causal association between two activities",
            "properties": {
                "downstream_activity": {
                    "type": "string"
                },
                "evidence": {
                    "items": {
                        "$ref": "#/$defs/EvidenceItem"
                    },
                    "type": "array"
